<div class="nested-fields well well-compact">
  <div class="form-group">
    <%= f.label :disc %>
    <%= f.text_field :disc_name %>
    <%= link_to_remove_association "remove disc", f, class: "btn btn-danger disc-remove" %>
    <div class="songs">
        <table>
          <%= f.fields_for :songs do |song| %>
            <%= render 'song_fields', f: song %>
        </table>
          <%end%>
        <div class="links">
          <%= link_to_add_association "add song", f, :songs, class: "btn btn-primary" %>
        </div>
      </div>
    </div>

