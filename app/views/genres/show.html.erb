
<body>
 <div class="content clearfix">

   <div class="main-content">
    <div class="title">
      <h3><strong>

        <%= @item %>

    </strong></h3></div>

      <div class="item-content">

        <% @items.each do |item| %>
          <div class="111">
            <div class="222">

              <%= link_to item_path(item.id) do %>

              <%= attachment_image_tag item, :item_image, :fill, 300, 300, format: 'jpg', class: "artist-item" %>

              <% end %>

            </div>
            <div class="artist-text">
              <div class="artist-name"><p><%= item.title %></p></div>
            </div>
          </div>
        <% end %>

      </div>
        <div class="pagenation">

        </div>
   </div>
 </div>
</body>
